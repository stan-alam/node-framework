{
    "id" : "1906",
    "name" : "Error Reporting Service",
    "testCases" : [
      {
        "testCaseId": 1,
        "name" : "Verify Insert of Error Service (Info)",
        "steps": [
          {
            "id": 1,
            "type": "error-reporting-service",
            "params": {
              "action": "sendErrorMessage"
            },
            "tests":[
              {
                "action": "equal",
                "value": 201,
                "location": "statusCode"
              },
              {
                "action": "isObject",
                "location": "body"
              }
            ]
          }
        ]
      },
      {
        "testCaseId": 2,
        "name" : "Verify Insert of Error Service (Warning)",
        "steps": [
          {
            "id": 1,
            "type": "error-reporting-service",
            "params": {
              "action": "sendErrorMessage",
              "updateModel": {
                "model": "errors",
                "attributes": {
                  "severity":"Warning",
                  "description":"This is description for Warning",
                  "details":"This is a more info on the Warning"
                }
              }
            },
            "tests":[
              {
                "action": "equal",
                "value": 201,
                "location": "statusCode"
              },
              {
                "action": "isObject",
                "location": "body"
              }
            ]
          }
        ]
      },
      {
        "testCaseId": 3,
        "name" : "Verify Insert of Error Service (Error)",
        "steps": [
          {
            "id": 1,
            "type": "error-reporting-service",
            "params": {
              "action": "sendErrorMessage",
              "updateModel": {
                "model": "errors",
                "attributes": {
                  "severity":"Error",
                  "description":"This is description for Error",
                  "details":"This is a more info on the Error"
                }
              }
            },
            "tests":[
              {
                "action": "equal",
                "value": 201,
                "location": "statusCode"
              },
              {
                "action": "isObject",
                "location": "body"
              }
            ]
          }
        ]
      },
      {
        "testCaseId": 4,
        "name": "Verify Insert of Error Service (Bad Media Type)",
        "steps": [
          {
            "id": 1,
            "type": "error-reporting-service",
            "params": {
              "action": "sendErrorMessageBadMediaType"
            },
            "tests": [
              {
                "action": "equal",
                "value": 415,
                "location": "statusCode"
              }
            ]
          }
        ]
      },
      {
        "testCaseId": 5,
        "name" : "Verify Insert of Error Service (Invalid token)",
        "steps": [
          {
            "id": 1,
            "type": "error-reporting-service",
            "params": {
              "action": "sendErrorMessageInvalidToken"
            },
            "tests":[
              {
                "action": "equal",
                "value": 401,
                "location": "statusCode"
              }
            ]
          }
        ]
      },
      {
        "testCaseId": 8,
        "name" : "Verify Insert of Error Service (Expired Token)",
        "steps": [
          {
            "id": 1,
            "type": "error-reporting-service",
            "params": {
              "action": "sendErrorMessageExpiredToken"
            },
            "tests":[
              {
                "action": "equal",
                "value": 401,
                "location": "statusCode"
              }
            ]
          }
        ]
      },
