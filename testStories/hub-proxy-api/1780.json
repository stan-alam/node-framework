        "steps": [
         {
            "id": 1,
            "type": "hub-proxy-api",
            "params": {
              "action": "getAccessToken",
              "sharedData": {
                  "key": "APIKey",
                  "sharelocation": {
                  "step": "preSetup",
                  "value": "integration.record2.apiKey"
                  }
               }
            }
          },       
          {
            "id": 2,
            "type": "hub-proxy-api",
            "params": {
              "action": "post"
             },
            "tests":[
              {
                "action": "equal",
                "value": 200
              }

            ]
          }
        ]
      },
      {
        "testCaseId": "PRX-TC02",
