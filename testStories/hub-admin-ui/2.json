{
    "id" : "2",
    "name" : "Adding Application using UI with special keys",
    "testCases" : [
      {
        "testCaseId": 1,
        "name" : "Verify Adding Applications",
        "steps": [
            {
                "id": 1,
                "type": "hub-admin-ui",
                "params": {
                    "action": "urllocation",
                    "location": "/#/dashboard"
                }
            },
            {
                "id": 2,
                "type": "hub-configuration-api",
                "params": {
                    "action": "deleteApplications",
                    "applications": [
                        "Application1"
                    ]
                }
            },
            {
                "id": 3,
                "type": "hub-admin-ui",
                "params": {
                    "action": "click",
                    "element": "manWizardAppCreate"
                }
            },
            {
                "id": 4,
                "type": "hub-admin-ui",
                    "params": {
                        "action": "sendkeys",
                        "element": "inputForApp",
                        "elementValue": "Application1"
                    }
            },
            {
                "id": 5,
                "type": "hub-admin-ui",
                "params": {
                    "action": "sendSpecialKey",
                    "element": "inputForApp",
                    "key": "TAB"
                }
            },
            {
                "id": 6,
                "type": "hub-admin-ui",
                "params": {
                    "action": "sendSpecialKey",
                    "element": "findElementIdByTemplate",
                    "elementTemplate": "wizard_save_button",
                    "key": "ENTER"
                }
            }
            ],
            "validation": [
            {
                "name": "Verify Application Name",
                "action": "elementView",
                "type": "hub-admin-ui",
                "element": "breadCrumbsOne",
                "test": {
                    "action": "include",
                    "value": "Application1"
                }
            }
        ]
      }
    ]
}
