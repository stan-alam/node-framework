{
    "id" : "2",
    "name" : "Adding Application using UI with special keys",
    "testCases" : [
      {
        "testCaseId": 1,
        "name" : "Verify Adding Applications",
        "steps": [
          {
            "id": 1,
            "method": "deleteApplications",
            "type": "hub-configuration-api",
            "params": {
              "action": "deleteApplications",
              "applications": [
                "Application1"
              ]
            }
          },
          {
            "id": 2,
            "method": "addApplicationMan",
            "type": "hub-admin-ui",
            "params": {
                "action": "click",
                "element": "manWizardAppCreate"
            }
          },
          {
            "id": 3,
            "method": "enterApplicationName",
            "type": "hub-admin-ui",
            "params": {
              "action": "sendkeys",
              "element": "inputForApp",
              "elementValue": "Application1"
            }
          },
          {
            "id": 4,
            "type": "hub-admin-ui",
            "params": {
              "action": "sendSpecialKey",
              "element": "inputForApp",
              "key": "TAB"
            }
        },
        {
            "id": 5,
            "type": "hub-admin-ui",
            "params": {
                "action": "sendSpecialKey",
                "element": "findElementIdByTemplate",
                "elementTemplate": "wizard_save_button",
                "key": "ENTER"
            }
        }
        ],
        "validation": [
          {
            "name": "Verify Application Name",
            "action": "elementView",
            "type": "hub-admin-ui",
            "element": "breadCrumbsOne",
            "test": {
              "action": "include",
              "value": "Application1"
            }
          }
        ]
      }
    ]
}
