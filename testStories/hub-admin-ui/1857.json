        {
            "testCaseId":6,
            "name":"Remove EPAs Credentials",
            "steps":[
                {
                    "id":1,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"urllocation",
                        "location":"/#/dashboard"
                    }
                },
                {
                    "id":2,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linkPrivilegedApp"
                    }
                },
                {
                    "id":3,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linkCredentialsTab"
                    }
                },
                {
                    "id":4,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"buttonDeleteAuthoritativeApplicationTemplate",
                        "elementTemplate":"Colleague",
                        "hoverElement":"cardAuthoritativeApplicationTemplate",
                        "hoverElementTemplate":"Colleague"
                    }
                },
                {
                    "id":5,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"buttonConfirmDelete"
                    }
                },
                {
                    "id":6,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"urllocation",
                        "location":"/#/dashboard"
                    }
                },
                {
                    "id":7,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linksViewApplication"
                    }
                },
                {
                    "id":8,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"dropdownPerPage",
                        "allowFail":true
                    }
                },
                {
                    "id":9,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"dropdownPerPageLast",
                        "allowFail":true
                    }
                },
                {
                    "id":10,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"elementView",
                        "element":"findAnchorByTemplate",
                        "elementTemplate":"Colleague"
                    },
                    "tests":[
                        {
                            "action":"equal",
                            "value":"Colleague"
                        }
                    ]
                }
            ]
        }
    ]
