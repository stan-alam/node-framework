{
    "id":"1857",
    "name":"Create credentials for EPAs in Ethos Integration for use by the Proxy service",
    "testCases":[
        {
            "testCaseId":1,
            "name":"Configure Pub and Sub applications and resources",
            "steps":[
                {
                    "id":1,
                    "type":"hub-configuration-api",
                    "params":{
                        "action":"deleteApplications",
                        "applications":[
                            "Banner",
                            "Colleague",
                            "Elevate",
                            "applicationPub"
                        ]
                    }
                },
                {
                    "id":2,
                    "type":"hub-configuration-api",
                    "params":{
                        "action":"createApplications",
                        "applications":[
                            "Banner",
                            "Colleague",
                            "Elevate",
                            "applicationPub"
                        ]
                    }
                },
                {
                    "id":3,
                    "type":"hub-configuration-api",
                    "params":{
                        "action":"addResource",
                        "application":"Banner",
                        "resources":"subjects"
                    }
                },
                {
                    "id":4,
                    "type":"hub-configuration-api",
                    "params":{
                        "action":"addResource",
                        "application":"Colleague",
                        "resources":"courses"
                    }
                },
                {
                    "id":5,
                    "type":"hub-configuration-api",
                    "params":{
                        "action":"addResource",
                        "application":"applicationPub",
                        "resources":[
                            "buildings",
                            "rooms",
                            "sites"
                        ]
                    }
                },
                {
                    "id":6,
                    "type":"hub-configuration-api",
                    "params":{
                        "action":"addSubscription",
                        "application":"Elevate",
                        "subscriptions":[
                            "subjects",
                            "courses",
                            "buildings",
                            "rooms",
                            "sites"
                        ]
                    }
                }
            ]
        },
        {
            "testCaseId":2,
            "name":"View EPAs Credentials List",
            "steps":[
                {
                    "id":1,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"urllocation",
                        "location":"/#/dashboard"
                    }
                },
                {
                    "id":2,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linkPrivilegedApp"
                    }
                },
                {
                    "id":3,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linkCredentialsTab"
                    }
                },
                {
                    "id":4,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"arrayValue",
                        "element":"authoritativeApplicationsCredentialsList"
                    },
                    "tests":[
                        {
                            "action":"includeMembers",
                            "value":[
                                "Banner",
                                "Colleague",
                                "applicationPub"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "testCaseId":3,
            "name":"EPAs Credentials Visual Cue",
            "steps":[
                {
                    "id":1,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"urllocation",
                        "location":"/#/dashboard"
                    }
                },
                {
                    "id":2,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linkPrivilegedApp"
                    }
                },
                {
                    "id":3,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linkCredentialsTab"
                    }
                },
                {
                    "id":4,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"buttonEditAuthoritativeApplicationTemplate",
                        "elementTemplate":"Banner",
                        "hoverElement":"cardAuthoritativeApplicationTemplate",
                        "hoverElementTemplate":"Banner"
                    }
                },
                {
                    "id":5,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"buttonEditUsername"
                    }
                },
                {
                    "id":6,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"sendkeys",
                        "element":"inputUsername",
                        "elementValue":"VisualCue"
                    }
                },
                {
                    "id":7,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"buttonSaveUsername"
                    }
                },
                {
                    "id":8,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"buttonEditPassword"
                    }
                },
                {
                    "id":9,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"sendkeys",
                        "element":"inputPassword",
                        "elementValue":"VisualCue"
                    }
                },
                {
                    "id":10,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"buttonSavePassword"
                    }
                },
                {
                    "id":11,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"click",
                        "element":"linkBackToCredentials"
                    }
                },
                {
                    "id":12,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"elementView",
                        "element":"credentialsUsernameTemplate",
                        "elementTemplate":"Banner"
                    },
                    "tests":[
                        {
                            "action":"equal",
                            "value":"VisualCue"
                        }
                    ]
                },
                {
                    "id":13,
                    "type":"hub-admin-ui",
                    "params":{
                        "action":"elementView",
                        "element":"credentialsPasswordTemplate",
                        "elementTemplate":"Banner"
                    },
                    "tests":[
                        {
                            "action":"equal",
                            "value":"*********"
                        }
                    ]
                }
            ]
        },
        {
